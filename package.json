{
  "name": "cursor-smart-router",
  "displayName": "Cursor智能路由插件",
  "description": "为Cursor Pro用户提供动态模型路由能力",
  "version": "1.0.0",
  "publisher": "cursor",
  "repository": {
    "type": "git",
    "url": "https://github.com/cursor/cursor-smart-router"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:cursor-smart-router.enable"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-smart-router.enable",
        "title": "启用智能路由"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.5",
    "vsce": "^2.7.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "node-cache": "^5.1.2",
    "winston": "^3.3.3"
  }
}