{
  "name": "cursor-smart-router",
  "displayName": "Cursor智能路由插件",
  "description": "为Cursor Pro用户提供动态模型路由能力",
  "version": "1.0.0",
  "publisher": "cursor",
  "repository": {
    "type": "git",
    "url": "https://github.com/cursor/cursor-smart-router"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-smart-router.showStatus",
        "title": "显示Cursor状态"
      },
      {
        "command": "cursor-smart-router.setApiToken",
        "title": "设置Cursor API Token"
      }
    ],
    "configuration": {
      "title": "Cursor智能路由",
      "properties": {
        "cursor-smart-router.apiEndpoint": {
          "type": "string",
          "default": "https://api.cursor.sh/v1",
          "description": "Cursor API端点"
        },
        "cursor-smart-router.updateInterval": {
          "type": "number",
          "default": 60,
          "description": "状态更新间隔（秒）"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.5",
    "vsce": "^2.7.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "node-cache": "^5.1.2",
    "winston": "^3.3.3"
  }
}